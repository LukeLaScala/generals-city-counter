
{
  "name": "Generals.io City Counter",
  "description": "Displays a city count next to each player on Generals.io",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["http://generals.io/*", "http://eu.generals.io/*"],
      "js": ["content_script.js"]
    }
  ],
  "manifest_version": 2
}
